    <script>
        function openApps() {
            // Open YouTube video
            window.location.href = "https://www.youtube.com/watch?v=1vrEljMfXYo"; 

            // Delay for 2 seconds, then open Google Maps
            setTimeout(function() {
                window.location.href = "google.navigation:q=Home";
            }, 2000);
        }
    </script>
